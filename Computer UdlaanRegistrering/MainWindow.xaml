<Window x:Class="Computer_UdlaanRegistrering.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Computer_UdlaanRegistrering"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top"  Orientation="Horizontal" Height="40" Width="200"  >
            <Button Content="Registrer udlån" Click="RegistrerUdlaanClick" Width="100"></Button>
            <Button Content="Oversigter" Click="VisOversigterClick" Width="100"/>
        </StackPanel>
        <Frame x:Name="ContentWindow" Margin="0,40,0,0" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
